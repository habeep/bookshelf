/* media queries */

@media only screen and ( min-width: 0px ) and ( max-width: 480px ){

body{
	background-image:url(../images/bg-small.jpg);
}
.main-wrapper{
	height:inherit;
	overflow:auto;
	overflow-x:hidden;
}
.wrapper{
	padding-top:12px;
	margin-left:7px;
}
.header-wrap{
	display:none;
}
.select-wrap{
	margin-top:13px;
}
.b-wrapper{
	margin-left:20px;
}
.testimonial-wrap{
	display:none;
}
.footer-wrap{
	display:none;
}
.select-wrap{
	display:none;
}

/* mobile */

.header-wrap-mobile{
