@charset "utf-8";
@import url('https://fonts.googleapis.com/css?family=Lato|Open+Sans');
@import url('https://fonts.googleapis.com/css?family=Varela+Round');
/* CSS Document */

/* bookshelf-library */

html,body
{
    width: 100%;
    height: 100%;
    margin: 0px;
    padding: 0px;
    overflow-x: hidden; 
}
body{
	margin:0px;
	border:0px;
	padding:0px;
	background-image:url(../images/bg-big1.jpg);
	background-position:top left;
	background-repeat:repeat-y;
	background-size:100% auto;
}
@font-face {
    font-family: 'rock';
    src: url(../images/ROCK.TTF);
}

/* main-page-layout */

.main-wrapper{
	width:100%;
	height:auto;
	margin:0px;
	padding:0px;
}
.wrapper{
	height:auto;
	margin: 0 auto;
	width:95%;
	padding-top:50px;
	margin-left:280px;
	position:relative;
	float:left;
}
.header-wrap{
	position:relative;
	float:left;
	width:100%;
	height:auto;
	margin: 0 auto;
}
.header-right-wrap{
	position:relative;
	float:left;
	height:auto;
	width:38%;
	margin: 0 auto;
	margin-top:23px;
}
.select-wrap{
	position:relative;
	float:left;
	width:100%;
	height:auto;
	margin: 0 auto;
	margin-top:53px;
}
.b-wrapper{
	height:auto;
	margin: 0 auto;
	padding-top:20px;
	margin-left:200px;
	position:relative;
	float:left;
}
.testimonial-wrap{
	height:auto;
	margin: 0 auto;
	padding-top:10px;
	margin-left:300px;
	width:75%;
	position:relative;
	float:left;
	background-image:url(../images/testimonial-bg.png);
	background-position:top left;
	background-repeat:no-repeat;
	margin-bottom:70px;
}
.footer-wrap{
	height:auto;
	margin: 0 auto;
	padding-top:10px;
	width:100%;
	position:relative;
	float:left;
	border-bottom:solid 2px #e34630;
}

/* logo */

.logo{
	margin:0px;
	padding:0px;
	border:0px;
	position:relative;
	float:left;
	cursor:pointer;
}
.logo img{
	cursor:hand;
	border:0px;
	max-width: 100%;
	height: auto;
	width: auto;
}

/* search */

.search-wrap{
	position:relative;
	float:left;
	font-family: 'rock';
	font-size:22px;
	font-weight: 200;
	width:75%;
}
.search {
	width: 100%;
	position: relative;
	float:left;
	background-color:transparent;
}
.searchTerm {
	float: left;
	width: 100%;
	padding:5px;
	border:0px;
	border-bottom:2px solid #e9e9e9;
	background-color:transparent;
	padding-bottom:18px;
	height: 35px;
	outline: none;
	font-family: 'rock';
	font-size:26px;
	font-weight: 100;
	color:#c7c7c7;
}
.searchTerm:focus{
	font-family: 'rock';
	font-size:26px;
	font-weight: 100;
	color:#c7c7c7;
}
.searchButton {
	position: absolute;  
	right: -50px;
	width: 40px;
	height: 45px;
	border:0px;
	border-bottom:2px solid #e9e9e9;
	background-image:url(../images/search-icon.png);
	background-color:transparent;
	text-align: center;
	cursor:pointer;
}

/* menu */

.long-menu{
	display:block;
	position:relative;
	float:left;
	font-family: 'rock';
	font-size:22px;
	font-weight: 200;
	color:#404040;
	width:100%;
	z-index:100;
	margin-top:15px;
}
.long-menu ul{
	list-style-type: none;
	margin:0px;
	padding:0px;
}
.long-menu ul li{
	list-style-type: none;
	display: inline-block;
	margin-right:30px;
}
.long-menu ul li a{
	text-decoration:none;
	font-family: 'rock';
	font-size:25px;
	font-weight: 200;
	color:#404040;
}
.long-menu ul li a:hover{
	color:#e34630;
}

/* cart*/

.cart{
	position:absolute;
	float:right;
	top:10px;
	right:152px;
	width:60px;
	height:53px;
	color:#b8b6b6;
	font-size:30px;
	background-color:transparent;
	text-align: center;
	padding-top:12px;
	cursor:pointer;
}
.cart-main{
	position:relative;
	float:left;
	top:30px;
	width:60px;
	height:53px;
	color:#e34630;
	font-size:33px;
	background-color:transparent;
	text-align: center;
	padding-top:12px;
	cursor:pointer;
}
.cart-full{
	position:absolute;
	float:right;
	top:15px;
	right:152px;
	width:60px;
	height:53px;
	color:#e34630;
	font-size:28px;
	background-color:transparent;
	text-align: center;
	padding-top:12px;
	cursor:pointer;
}
.cart-main a{
	color:#e34630;
	text-decoration:none;
}

/* sticky */

.select-wrap-sticky{
	position: -webkit-sticky;
	position: sticky;
	top: 0;
	width:100%;
	height:auto;
	background-color:#ffffff;
	box-shadow: 0 8px 6px -6px #b8b6b6;
	margin: 0 auto;
	padding-top:18px;
	padding-bottom:6px;
	padding-left:20px;
	z-index:999;
}
.sticky-padding{
	display:flex;
	flex-direction:row;
	justify-content:flex-start;
	flex-flow: row wrap;
	align-items:left;
}


/* select */

.styled-select select {
	background-color: transparent;
	width: 190px;
	padding:8px;
	font-size: 16px;
	color:#343434;
	font-family: "Lato", "Open Sans", sans-serif;
	font-weight:bold;
	border: 0;
	border-radius: 0;
	height: 34px;
	-webkit-appearance: none;
}
.styled-select {
	width: 190px;
	height: 34px;
	overflow: hidden;
	background: url(../images/select_arrow.png) no-repeat right ;
	border:1px solid #c9c9c9;
	position:relative;
	float:left;
	margin-right:12px;
	margin-bottom:22px;
}
.selectButton {
	position:relative;
	float:left;
	width:35px;
	height:44px;
	background-image:url(../images/search-icon.png);
	background-size:100% auto;
	background-repeat:no-repeat;
	background-position:center;
	background-color:transparent;
	text-align: center;
	cursor:pointer;
}
.selectButton-responsive{
	display:none;
}

/* select mobile */

.select-mobile-wrap{
	display:none;
}

/* pricing newly added feb 9 2019 */

.price-wrap{
	margin: 0 auto;
	padding:0px;
	border:0px;
	width:92%;
	margin-left:80px;
	position:relative;
	float:left;
	margin-bottom:20px;
	margin-top:20px;
}
.price-desc{
	position:relative;
	float:left;
	width:98%;
	display:flex;
	flex-direction:column;
	justify-content:flex-start;
	flex-flow: row wrap;
	align-items:left;
}
.price-desc-left{
	position:relative;
	float:left;
	font-size:36px;
	color:#e34630;
	font-family: 'Varela Round', sans-serif;
	font-weight:bold;
	text-transform:capitalize;
	width:30%;
}
.price-desc-left div{
	padding-bottom:3px;
	border-bottom:solid 3px #e34630;
	margin-right:30px;
	width:85%;
}
.price-desc-right{
	position:relative;
	float:left;
	font-size:30px;
	color:#504848;
	font-family: 'Varela Round', sans-serif;
	width:50%;
	padding-top:6px;
}
.price-desc-right span{
	font-weight:bold;
	color:#000000;
}
.price-opt{
	position:relative;
	float:left;
	width:98%;
	display:flex;
	flex-direction:row;
	justify-content:flex-start;
	flex-flow: row wrap;
	align-items:left;
	margin-top:50px;
}
.member-charges{
	position:relative;
	float:left;
	width:98%;
	font-size:33px;
	color:#000000;
	font-family: 'Varela Round', sans-serif;
	font-weight:bold;
}
.member-charges span{
	color:#fe3000;
}
.member-benefits{
	position:relative;
	float:left;
	width:98%;
	margin-top:10px;
	font-size:24px;
	color:#504848;
	font-family: 'Varela Round', sans-serif;
	font-weight:bold;
}
.member-benefits ul{
	list-style-type: none;
	margin:0px;
	padding:0px;
}
.member-benefits ul li{
	list-style-type: none;
	display: inline;
	width:95%;
	line-height:45px;
	color:#000000;
	margin-right:15px;
}
.member-benefits ul li:before{
	content: '\f00c'; /* You should use \ and not /*/
	font-family: "Font Awesome 5 Free"; /* This is the correct font-family*/
	padding-right:5px;
	font-weight:bold;
	color:#fe3000;
	font-size:22px;
}
.member-subscribe{
	position:relative;
	float:left;
	width:98%;
	margin-top:30px;
	margin-bottom:120px;
}
.member-button{
	text-align: center;
    cursor:pointer;
    color:#ffffff;
    background-color:#fe3000;
    box-sizing: border-box;
    padding:12px 15px 12px;
    border-width:0px;
    border-style:none;
	font-size:21px;
	color:#ffffff;
	font-family: "Lato", "Open Sans", sans-serif;
	font-weight:bold;
}

/* if needed select box css for pricing */
.price-select {
	width:320px;
	height:64px;
	overflow: hidden;
	background: url(../images/select_arrow.png) no-repeat right ;
	border:4px solid #c9c9c9;
	position:relative;
	float:left;
	margin-right:12px;
	margin-bottom:22px;
}
.price-select select {
	background-color: transparent;
	width:360px;
	height:64px;
	padding:8px;
	font-size:26px;
	color:#343434;
	font-family: "Lato", "Open Sans", sans-serif;
	font-weight:bold;
	border: 0;
	border-radius: 0;
	-webkit-appearance: none;
}
/* if needed select box css for pricing ends*/

/* book */

.book-wrap{
	margin: 0 auto;
	padding:0px;
	border:0px;
	width:95%;
	position:relative;
	float:left;
	margin-bottom:20px;
}
.book{
	position:relative;
	float:left;
	width:307px;
	margin-right:22px;
	margin-bottom:65px;
}
.book-image{
	position:relative;
	float:left;
	width:307px;
	border:1px solid #cacaca;
	cursor:pointer;
}
.book-image img{
	opacity: 1;
	transition: opacity .25s ease-in-out;
	-moz-transition: opacity .25s ease-in-out;
	-webkit-transition: opacity .25s ease-in-out;
}
.book-image:hover img{
	opacity: 0.8;
}
.book-details{
	position:relative;
	float:left;
	width:307px;
	border:1px solid #cacaca;
}
.book-title-wrap{
	position:relative;
	float:left;
	width:232px;
	margin-left:17px;
	margin-top:11px;
	margin-bottom:17px;
}
.book-title{
	position:relative;
	float:left;
	width:232px;
	font-family: "Lato", "Open Sans", sans-serif !important;
	font-size:18px;
	font-weight:bold;
	color:#131313;
	margin-bottom:5px;
}
.book-title a{
	text-decoration:none;
	color:#131313;
}
.book-title a{
	text-decoration:none;
	color:#131313;
}
.book-age{
	position:relative;
	float:left;
	width:232px;
	font-family: "Lato", "Open Sans", sans-serif;
	font-size:14px;
	font-weight:bold;
	color:#131313;
	margin-bottom:20px;
}
.book-author{
	position:relative;
	float:left;
	width:232px;
	font-family: "Lato", "Open Sans", sans-serif;
	font-size:14px;
	font-weight:bold;
	font-style:italic;
	text-transform:lowercase;
	color:#131313;
	margin-bottom:5px;
}
.book-rent-wrap{
	position:relative;
	float:left;
	width:307px;
}
.book-publisher{
	position:relative;
	float:left;
	width:222px;
	font-family: "Lato", "Open Sans", sans-serif;
	font-size:14px;
	font-style:italic;
	text-transform:lowercase;
	font-weight:bold;
	color:#131313;
}
.book-rent{
	position:relative;
	float:left;
	width:55px;
	cursor:pointer;
	padding-right:12px;
}
.book-rent img{
	max-width: 100%;
	-moz-transition: all 0.1s;
	-webkit-transition: all 0.1s;
	transition: all 0.1s;
}
.book-rent:hover img{
	-moz-transform: scale(1.1);
	-webkit-transform: scale(1.1);
	transform: scale(1.1);
}

/* testimonial */

.testimonial-wrap{
	height:auto;
	margin: 0 auto;
	padding-top:10px;
	margin-left:300px;
	width:75%;
	position:relative;
	float:left;
	background-image:url(../images/testimonial-bg.png);
