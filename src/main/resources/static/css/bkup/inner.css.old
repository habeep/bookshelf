/* expandable search */
/*https://codepen.io/CBeghin/pen/HeuiF*/

.search-expand{
	position:relative;
	float:right;
	padding-top:9px;
}
#wrap {
	margin:0;
	display: inline-block;
	position: relative;
	float:right;
	margin-right:12px;
	height:30px;
	float: right;
	padding: 0;
}
.search-expand input[type="text"] {
	height:20px;
	display: inline-block;
	color:#000000;
	font-family: "Lato", "Open Sans", sans-serif;
	font-size:13px;
	font-weight:bold;
	border: none;
	outline: none;
	color: #555;
	padding-top:4px;
	padding-bottom:3px;
	padding-left:7px;
	padding-right:30px;
	width: 0px;
	position: absolute;
	top: 0;
	right: 0;
	background: none;
	z-index: 3;
	transition: width .4s cubic-bezier(0.000, 0.795, 0.000, 1.000);
	cursor: pointer;
}
.search-expand input[type="text"]:focus:hover {
	border: 1px solid #c8c8c8;
	border-radius:12px;
}
.search-expand input[type="text"]:focus {
	width:150px;
	z-index: 1;
	border: 1px solid #e8e8e8;
	border-radius:12px;
	cursor: text;
}
.search-expand input[type="submit"] {
	height:18px;
	width: 33px;
	margin-top:6px;
	margin-bottom:6px;
	margin-right:4px;
	outline:none;
	display: inline-block;
	float: left;
	background:url(../images/m-search-icon.png);
	background-repeat:no-repeat;
	background-position:right  center;
	background-size:70%;
	background-color:transparent;
	text-indent: -10000px;
	border: none;
	position: absolute;
	top:0;
	right: 0;
	z-index: 2;
	cursor: pointer;
	opacity: 0.4;
	cursor: pointer;
	transition: opacity .4s ease;
}
.search-expand input[type="submit"]:hover {
	opacity: 0.8;
}

